	<queryString language="SQL">
		<![CDATA[SELECT
     tipo_dispositivo.`Id_tipo_dispositivo` AS tipo_dispositivo_Id_tipo_dispositivo,
     tipo_dispositivo.`Tipo_dispositivo` AS tipo_dispositivo_Tipo_dispositivo,
     dispositivo.`Serial_dispositivo` AS dispositivo_Serial_dispositivo,
     dispositivo.`Marca_dispositivo` AS dispositivo_Marca_dispositivo,
     dispositivo.`Modelo_dispositivo` AS dispositivo_Modelo_dispositivo,
     dispositivo.`Id_tipo_dispositivo` AS dispositivo_Id_tipo_dispositivo,
     dispositivo.`Id_persona_reg` AS dispositivo_Id_persona_reg,
     dispositivo.`Id_vigilante` AS dispositivo_Id_vigilante,
     dispositivo.`Fecha_registro` AS dispositivo_Fecha_registro,
     persona.`Id_persona_reg` AS persona_Id_persona_reg,
     persona.`Nombre_persona` AS persona_Nombre_persona,
     persona.`Apellido_persona` AS persona_Apellido_persona,
     persona.`Ficha_persona` AS persona_Ficha_persona,
     persona.`Id_tipo_persona` AS persona_Id_tipo_persona,
     ingreso_dispositivo.`cons_ing_dispositivo` AS ingreso_dispositivo_cons_ing_dispositivo,
     ingreso_dispositivo.`Id_persona_ing` AS ingreso_dispositivo_Id_persona_ing,
     ingreso_dispositivo.`Serial_dispositivo` AS ingreso_dispositivo_Serial_dispositivo,
     ingreso_dispositivo.`Fecha_ingreso` AS ingreso_dispositivo_Fecha_ingreso,
     ingreso_dispositivo.`Fecha_salida` AS ingreso_dispositivo_Fecha_salida
FROM
     `tipo_dispositivo` tipo_dispositivo INNER JOIN `dispositivo` dispositivo ON tipo_dispositivo.`Id_tipo_dispositivo` = dispositivo.`Id_tipo_dispositivo`
     INNER JOIN `persona` persona ON dispositivo.`Id_persona_reg` = persona.`Id_persona_reg`
     INNER JOIN `ingreso_dispositivo` ingreso_dispositivo ON persona.`Id_persona_reg` = ingreso_dispositivo.`Id_persona_ing`]]>
	</queryString>
